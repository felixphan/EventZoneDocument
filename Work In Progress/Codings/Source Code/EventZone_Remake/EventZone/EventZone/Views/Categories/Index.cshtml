@model IEnumerable<EventZone.Models.Category>

@foreach (var item in Model)
{
    using (Html.BeginForm("CategorySearch","Search"))
    {
        <li>
            <button type="submit" class="btn btn-default">@Html.DisplayFor(modelItem => item.CategoryName)</button>
            @Html.HiddenFor(modelItem=> item.CategoryID)
        </li>
    }
}
